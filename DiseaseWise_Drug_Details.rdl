  <DataSets>
    <DataSet Name="Drug">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select distinct A.dr_id,C.name as drug_name,C.type,b.name as disease_name 
from [AdventureWorksDW2016].[dbo].[CLINICAL_FACT_1_5370] A
left join DISEASE_DIM_1_5370 B on A.ds_id=B.ds_id
left join DRUG_DIM_1_5370 C on cast(A.dr_id as varchar(50))=cast(C.dr_id as varchar(50) )</CommandText>
      </Query>
